<p id="notice"><%= notice %> <%= link_to 'Create New Session', new_session_path %></p>

<p>
  <b>Title:</b>
  <%= @session.title %>
</p>

<p>
  <b>Start:</b>
  <%= @session.start %>
</p>

<p>
  <b>End:</b>
  <%= @session.end %>
</p>

<% unless @session.location.nil? %>
<p>
  <b>Location:</b>
  <%= @session.location.name %>
</p>
<% end %>

<% unless @session.session_type.nil? %>
<p>
  <b>Session type:</b>
  <%= @session.session_type.title %>
</p>
<% end %>

<p>
  <b>Description:</b>
  <%= @session.description %>
</p>


<%= link_to 'Edit', edit_session_path(@session) %> |
<%= link_to 'Back', sessions_path %>
