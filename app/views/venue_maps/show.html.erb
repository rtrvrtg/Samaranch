<p id="notice"><%= notice %></p>

<div id="map-container" data-mapid="<%= @venue_map.id %>">
<%= image_tag(@venue_map.map_image.url, :class => "map-image") %>
</div>

<div class="row-fluid">

<div class="span4">
<p>
  <b>Map name:</b>
  <%= @venue_map.map_name %>
</p>
</div>

<div class="span4">
<p>
  <b>Venue:</b>
  <%= link_to @venue_map.venue.name, venue_path(@venue_map.venue) %>
</p>
</div>

<div class="span4">
<%= link_to 'Edit', edit_venue_map_path(@venue_map) %> |
<%= link_to 'Back', venue_maps_path %>
</div>

</div>

<script type="text/javascript">
$(document).ready(function(){

var VML = new Samaranch.VenueMapDisplay({
	container: 'map-container'
});

});
</script>
