<h1>Listing Session Types</h1>

<table class="table table-bordered table-striped">
<thead>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
</thead>
<tbody>
<% @session_types.each do |session_type| %>
  <tr>
    <td><%= session_type.title %></td>
    <td><%= session_type.description %></td>
    <td><%= link_to 'Show', session_type %></td>
    <td><%= link_to 'Edit', edit_session_type_path(session_type) %></td>
    <td><%= link_to 'Destroy', session_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</tbody>
<tfoot>
	<tr><td colspan="4">
		<%= link_to 'New Session type', new_session_type_path %>
	</td></tr>
</tfoot>
</tbody>
</table>


